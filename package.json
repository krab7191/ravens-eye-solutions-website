{
  "name": "ravens-eye-solutions-website",
  "homepage": "https://github.com/krab7191/ravens-eye-solutions-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "classnames": "^2.2.6",
    "gh-pages": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "build:css": "postcss src/**/*.css -r --verbose",
    "watch:css": "postcss src/**/*.css -r --verbose",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "react-scripts start",
    "build": "npm-run-all build:css build-js",
    "build-js": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "postcss-cli": "^6.1.2",
    "postcss-nested": "^4.1.2"
  }
}
